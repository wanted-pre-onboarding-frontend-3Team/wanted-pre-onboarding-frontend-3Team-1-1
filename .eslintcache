>\n      <TextInput type=\"email\" title=\"아이디\" message=\"이메일 형식으로 입력해 주세요.\" required {...register('email')} />\n      <TextInput\n        type=\"password\"\n        title=\"비밀번호\"\n        message=\"최소 8자 이상 입력해 주세요.\"\n        minLength={8}\n        required\n        {...register('password')}\n      />\n      <Button disabled={!validation}>회원가입</Button>\n    </StyledForm>\n  );\n};\n\nconst StyledForm = styled.form`\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n`;\n\nexport default JoinForm;\n","/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/components/auth/LoginForm.js",["126"],[],"import styled from 'styled-components';\nimport TextInput from '../TextInput';\nimport useForm from '../../hooks/useForm';\nimport Button, { BUTTON_COLOR_WHITE } from '../Button';\nimport { useCallback } from 'react';\nimport { useModelContext } from '../../store/ModelContext';\nimport { useNavigate } from 'react-router-dom';\nimport Label from '../Label';\n\nconst INIT_FORM = {\n  email: '',\n  password: '',\n};\n\nconst LoginForm = () => {\n  const navigate = useNavigate();\n\n  const { account, isSuccess, isError } = useModelContext();\n  const { register, form, validation } = useForm(INIT_FORM);\n\n  const requestLogin = useCallback(\n    async (e) => {\n      e.preventDefault();\n\n      const response = await account.login(form);\n\n      if (isSuccess(response)) {\n        localStorage.setItem('token', response.data.access_token);\n        navigate('/', { replace: true });\n      } else if (isError(response)) {\n        alert('로그인 실패, 아이다나 비밀번호를 확인해 주세요.');\n      }\n    },\n    [account, form, isError, isSuccess, navigate],\n  );\n\n  return (\n    <StyledForm onSubmit={requestLogin}>\n      <Label>로그인</Label>\n      <TextInput type=\"text\" title=\"아이디\" required {...register('email')} />\n      <TextInput type=\"password\" title=\"비밀번호\" required {...register('password')} />\n\n      <Button disabled={!validation}>로그인</Button>\n      <Button to=\"/auth?page=join\" color={BUTTON_COLOR_WHITE}>\n        회원가입\n      </Button>\n    </StyledForm>\n  );\n};\n\nconst StyledForm = styled.form`\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n`;\n\nexport default LoginForm;\n","/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/hooks/useForm.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/hooks/useMount.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/index.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/model/AccountModel.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/model/BaseModel.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/pages/Auth.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/pages/Main.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/pages/Todo.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/routes.js",[],[],"/Users/anna_home/Documents/GitHub/wanted-pre-onboarding-frontend/wanted-pre-onboarding-frontend-1/wanted-pre-onboarding-frontend-3Team-1-1/src/store/ModelContext.js",[],[],"/Users/anna